/* pages/withdraw/withdrawHistory/withdrawHistory.wxss */
page{
  background-color: #F5EFEF;
  opacity: 100%
}
.historyContainer{
  width: 100%;
  font-size: 28rpx;
  color:#000;
}
.historyContainer .historyTop{
  margin: 0 30rpx 0;
  height: 70rpx;
}
.historyContainer .historyTop image{
  width: 690rpx;
  height: 51rpx;
  margin-top: 36rpx;
}
.historyContainer .historyInfo{
  width: 674rpx;
  height: 68rpx;
  margin: 0 38rpx;
  background-color: #fff;
  opacity: 50%;
  border-bottom: #F5EFEF 2rpx solid;
  text-align: center;
}
.historyContainer .historyInfo text {
  display: inline-block;
  width: 224rpx;
}
.historyContainer .historybottom{
  width: 674rpx;
  min-height: 600rpx;
  margin: 0 38rpx;
  background-color: #fff;
  opacity: 10%;
  box-shadow: 0rpx 3rpx 3rpx #f5c7c7;
  text-align: center;
}
.historyContainer .noHistory{
  text-align: center;
  line-height: 500rpx;
  color:#a3a3a3;
}
.historyContainer .historybottom .historyList{
  width: 674rpx;
  margin: 0 auto;
  padding: 36rpx 0;
  border-bottom: #F5EFEF 2rpx solid;
  text-align: center;
}
.historyContainer .historybottom text{
  width: 224rpx;
  color: #000;
  display: inline-block;
}
.historyContainer .historybottom .check text.statue{
  color:red;
}
.historyContainer .historybottom .success text{
  color:#a3a3a3;
}
.historyBottomImg{
  width: 678rpx;
  margin: 0 36rpx 0 36rpx;
}
.historyBottomImg image{
  width: 100%;
  height: 50rpx;
}