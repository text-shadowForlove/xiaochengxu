/* pages/withdraw/withdrawHistory/withdrawHistory.wxss */
page{
  background-color: #F5EFEF;
  opacity: 100%
}
.historyContainer{
  width: 100%;
  font-size: 28rpx;
  color:#000;
}
.historyContainer .historyTop{
  margin: 0 30rpx 0;
  height: 70rpx;
}
.historyContainer .historyTop image{
  width: 690rpx;
  height: 51rpx;
  margin-top: 36rpx;
}
.historyContainer .historyInfo{
  width: 674rpx;
  height: 68rpx;
  margin: 0 38rpx;
  background-color: #fff;
  opacity: 50%;
  border-bottom: #F5EFEF 2rpx solid;
}
.historyContainer .historyInfo text.date{
  margin-left: 114rpx;
  margin-right: 121rpx;
}
.historyContainer .historyInfo text.statue{
  margin-left: 118rpx;
  margin-right: 110rpx;
}
.historyContainer .historybottom{
  width: 674rpx;
  min-height: 600rpx;
  margin: 0 38rpx;
  background-color: #fff;
  opacity: 10%;
  box-shadow: 0rpx 3rpx 3rpx #f5c7c7;
}
.historyContainer .noHistory{
  text-align: center;
  line-height: 500rpx;
  color:#a3a3a3;
}
.historyContainer .historybottom .historyList{
  width: 630rpx;
  margin: 0 auto;
  padding: 36rpx 0;
  border-bottom: #F5EFEF 2rpx solid;
}
.historyContainer .historybottom text{
  width: 130rpx;
  text-align: center;
  color: #000;
}
.historyContainer .historybottom text.date{
  margin-left: 52rpx;
  margin-right: 90rpx;
}
.historyContainer .historybottom text.statue{
  margin-left: 120rpx;
  margin-right: 66rpx
}
.historyContainer .historybottom .check text.statue{
  color:red;
}
.historyContainer .historybottom .success text{
  color:#a3a3a3;
}
.historyBottomImg{
  width: 678rpx;
  margin: 0 36rpx 0 36rpx;
}
.historyBottomImg image{
  width: 100%;
  height: 50rpx;
}